<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<MessagePopover id="idMessagePopover" items="{message>/}">
		<items>
			<MessageItem
					type="{message>type}"
					title="{message>message}"
					description="{message>description}"
					id="idMessageItem"
			/>
		</items>
	</MessagePopover>
</core:FragmentDefinition>
