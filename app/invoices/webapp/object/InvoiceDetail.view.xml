<mvc:View
    controllerName="bstu.hmss.manageinvoices.object.InvoiceDetail"
    xmlns="sap.m"
    displayBlock="true"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout"
    height="100%"
    xmlns:facet="bstu.hmss.lib.control.facet">
    <f:DynamicPage id="idInvoiceDetailPage"
                   class="sapUiNoContentPadding"
                   stickySubheaderProvider="iconTabBar">
        <f:title>
            <f:DynamicPageTitle id="idInvoiceDetailPageTitle">
                <f:heading>
                    <Title text="{parts: [{path:'i18n>invoiceObjectPageTitle'}, {path:'this>InvoiceNumber'}],
                                formatter: '.formatter.formatMessage'}"
                           id="idInvoiceDetailTitle"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true" id="idInvoiceDetailPageHeader">

            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <IconTabBar
                id="iconTabBar"
                selectedKey="INVOICEITEMS"
                backgroundDesign="Transparent"
                class="sapUiResponsiveContentPadding"
                expandable="false"
                stretchContentHeight="true">
                <items>
                    <IconTabFilter
                        id="INVOICEITEMS"
                        key="INVOICEITEMS"
                        text="{i18n>InvoiceItems.Title}">
                        <mvc:XMLView
                            id="idInvoiceItemListView"
                            viewName="bstu.hmss.manageinvoices.object.tabs.items.InvoiceItemList"/>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </f:content>
    </f:DynamicPage>
</mvc:View>
